int y=300;
int z=550;
PImage photo;
void setup(){ 
    size(600,600); 
    photo=loadImage("spaceship.png");
    }
    void draw(){  
background(0);  
image(photo,y,z);  
}
void keyPressed(){
if (key == CODED){     
if (keyCode == DOWN){
          z=z+4;
          
          
if (key == CODED){     
    if (keyCode == UP){      
        z=z-4;     
        }    
        } 
        }   
        }     
        }

